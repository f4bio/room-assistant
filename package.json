{
  "name": "room-assistant",
  "version": "4.0.0",
  "description": "Presence tracking and more for automation on the room-level",
  "author": "Heiko Rothe <me@heikorothe.com> (https://heikorothe.com)",
  "license": "MIT",
  "repository": "github:mKeRix/room-assistant",
  "keywords": [
    "smart home",
    "home automation",
    "home assistant",
    "presence",
    "bluetooth",
    "thermopile"
  ],
  "homepage": "https://www.room-assistant.io",
  "bugs": "https://github.com/mKeRix/room-assistant/issues",
  "funding": "https://github.com/mKeRix/room-assistant?sponsor=1",
  "bin": "./bin/room-assistant.js",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "files": [
    "dist/**/*.{js,ts}",
    "dist/config/definitions/*.yml",
    "bin/*.js",
    ".github/FUNDING.yml"
  ],
  "scripts": {
    "prepack": "npm run build",
    "prebuild": "npx rimraf dist",
    "build": "npx nest build",
    "postinstall": "npx github-sponsors",
    "format": "npx prettier --write \"src/**/*.ts\" \"bin/*.js\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npx eslint '{src,apps,libs,test,bin}/**/*.{ts,js}'",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:cov": "npx jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs"
  },
  "dependencies": {
    "@mkerix/noble": "^1.9.2-11.1",
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/platform-ws": "^8.0.4",
    "@nestjs/schedule": "^1.0.0",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/websockets": "^8.0.4",
    "@willsoto/nestjs-prometheus": "^4.0.0",
    "2d-array-rotation": "^2.1.1",
    "async-mqtt": "^2.6.1",
    "async-mutex": "^0.3.1",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "config": "^3.3.6",
    "democracy": "^3.1.3",
    "github-sponsors": "^1.0.1",
    "joiful": "^3.0.0",
    "js-yaml": "^4.0.0",
    "kalmanjs": "^1.1.0",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.0",
    "nest-emitter": "^1.1.1",
    "nest-winston": "^1.6.0",
    "onoff": "^6.0.3",
    "please-upgrade-node": "^3.2.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "slugify": "^1.5.0",
    "swagger-ui-express": "^4.1.6",
    "systeminformation": "^5.6.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/i2c-bus": "^5.1.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/mdns": "^0.0.38",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "prettier": "2.5.1",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.2",
    "webpack": "^5.53.0"
  },
  "peerDependencies": {
    "winston-elasticsearch": "^0.16.1",
    "winston-loki": "^6.0.5"
  },
  "peerDependenciesMeta": {
    "winston-elasticsearch": {
      "optional": true
    },
    "winston-loki": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12 < 21"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./env.ts"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
